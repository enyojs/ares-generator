{
	"name": "ares-generator",
	"main": "./ares-generator.js",
	"version": "0.3.4",
	"description": "Project-generation toolkit for the ares-ide EnyoJS IDE",
	"keywords": ["ares-ide", "enyo"],
	"homepage": "https://github.com/enyojs/ares-generator",
	"bugs": {
		"url": "https://enyojs.atlassian.net/browse/ENYO"
	},
	"repository" : { 
		"type" : "git",
		"url" : "http://github.com/enyojs/ares-generator.git"
	},
	"engines": {
		"node": "~0.10.0",
		"npm": ">=1.2.11"
	},
	"engineStrict": true,
	"os" : [ "darwin", "linux", "win32" ],
	"dependencies": {
		"extract-zip": "~1.0.3",
		"async": "~0.2.9",
		"graceful-fs": "~2.0.1",
		"mkdirp": "~0.3.5",
		"npmlog": "~0.0.4",
		"request": "~2.26.0",
		"rimraf": "~2.2.2",
		"temp": "~0.6.0",
		"rsync": "^0"
	},
	"bundledDependencies": [
		"extract-zip",
		"async",
		"graceful-fs",
		"mkdirp",
		"npmlog",
		"request",
		"rimraf",
		"temp",
		"rsync"
	],
	"devDependencies": {
		"mocha": "~1.4.2",
		"shelljs": "~0.1.4",
		"should": "~1.1.0",
		"jshint": "~2.1.3",
		"nopt": "~2.1.2"
	},
	"scripts" : {
		"install" : "node ./scripts/sub-npm.js",
		"pretest" : "node ./node_modules/.bin/jshint .",
		"Xpretest": "node_modules/.bin/mocha test/generator.spec.js --level silly"
	},
	"files": [
		"LICENSE-2.0.txt",
		"README.md",
		"ares-generator.js",
		"copyFile.js",
		"copyDir.js",
		"scripts"
	]
}
